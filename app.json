{
  "expo": {
    "scheme": "digitalquote",
    "name": "Digital Quote",
    "slug": "digitalquote",
    "platforms": ["ios", "android", "web"],
    "version": "1.2.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.Rocket.DigitalQuote"
    },
    "android": {
      "package": "com.Rocket.DigitalQuote",
      "googleServicesFile": "./google-services.json",
      "versionCode": 4
    }
  }
}
